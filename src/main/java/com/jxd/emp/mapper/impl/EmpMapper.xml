<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--标准的结构化文档-->

<!--namespace接口的全路径，用于关联dao层接口-->
<mapper namespace="com.jxd.emp.mapper.IEmpMapper">

    <select id="getAll" resultType="com.jxd.emp.model.Emp">
        select empno,ename,job,sal,hiredate,dname,deptno from emp
    </select>



</mapper>